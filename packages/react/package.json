{
  "name": "@wfp/react",
  "version": "1.19.3",
  "description": "WFP UI Kit",
  "license": "Apache-2",
  "main": "lib/index.js",
  "module": "es/index.js",
  "main:scss": "source/globals/styles.scss",
  "types": "es/dist/index.d.ts",
  "scripts": {
    "build": "yarn rollup -c --bundleConfigAsCjs && yarn build:demos && yarn generate:types",
    "clean": "rimraf es lib umd storybook-static build react-docgen.json",
    "build:rollup": "rollup -c --bundleConfigAsCjs",
    "build:demos": "rollup -c rollupDemos.config.js --bundleConfigAsCjs",
    "build:storybook": "rm -r -f docs && storybook build -o docs",
    "generate:types": "rollup --config generateTypeDefinitions/rollup.config.js --bundleConfigAsCjs && node .temp/generateTypeDefinitions.js && rm -rf .temp",
    "dev": "npm run storybook",
    "storybook": "STORYBOOK_ASSETS=https://cdn.wfp.org/guides/ui/assets/v0.0.1/ STORYBOOK_NPM_VERSION=10.10.3 storybook dev -p 9000",
    "test": "jest",
    "jest-w": "jest --watch"
  },
  "sass": {
    "includePaths": [
      "./node_modules"
    ]
  },
  "keywords": [
    "WFP",
    "World Food Programme",
    "UN",
    "United Nations",
    "react",
    "ui-kit",
    "components"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "dev-expression",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-export-default-from"
    ]
  },
  "files": [
    ".storybook",
    "build",
    "config",
    "es",
    "lib",
    "src",
    "umd",
    "rollup.config.ts",
    "react-docgen.json"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
